{
  "name": "shrikeh/adr-contracts",
  "description": "Contracts for Action Domain Responder (ADR) projects",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Barney Hanlon",
      "email": "barney@shrikeh.net"
    }
  ],
  "require": {
    "shrikeh/common-app": "@dev"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "google/protobuf": "^4.29",
    "nyholm/psr7": "^1.8",
    "phpbench/phpbench": "^1.3",
    "phpstan/phpdoc-parser": "^1.31",
    "psr/http-message": "^2.0",
    "shrikeh/app-symfony": "@dev",
    "shrikeh/php-coding-bible": ">=0.2.0",
    "shrikeh/teapot": "^3.0",
    "shrikeh/symfony-api-context": "@dev",
    "shrikeh/testing-metapackage": ">=0.3",
    "symfony/console": "^7.2",
    "symfony/dotenv": "*",
    "symfony/runtime": "^7.2"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/shrikeh/common-app"
    },
    {
      "type": "vcs",
      "url": "https://github.com/shrikeh/app-symfony"
    },
    {
      "type": "vcs",
      "url": "https://github.com/shrikeh/symfony-api-context"
    }
  ],
  "autoload": {
    "psr-4": {
      "Examples\\": "examples/src",
      "Shrikeh\\AdrContracts\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "pestphp/pest-plugin": true,
      "phpro/grumphp-shim": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "extra": {
    "bin-dir": "app/bin",
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    },
    "config-dir": "app/config",
    "public-dir": "app/public",
    "src-dir": "app/src",
    "var-dir": "build/var"
  }
}
