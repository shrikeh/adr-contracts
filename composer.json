{
  "name": "shrikeh/adr-contracts",
  "description": "Contracts for Action Domain Responder (ADR) projects",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Barney Hanlon",
      "email": "barney@shrikeh.net"
    }
  ],
  "require": {
    "shrikeh/cqrs-contracts": "@dev"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "phpbench/phpbench": "^1.3",
    "phpstan/phpdoc-parser": "^1.31",
    "shrikeh/testing-metapackage": ">=0.3",
    "symfony/dotenv": "*",
    "symfony/runtime": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "Shrikeh\\AdrContracts\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "pestphp/pest-plugin": true,
      "phpro/grumphp-shim": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  },
  "repositories": {
    "shrikeh/cqrs-contracts": {
      "type": "vcs",
      "url": "git@github.com:shrikeh/cqrs-contracts.git"
    }
  }
}
